{
  "name": "web",
  "version": "1.0.0",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "build": "parcel build *.html",
    "prebuild": "(test -d dist || mkdir dist) && cp -rv scripts dist",
    "prestart": "yarn prebuild",
    ".": "parcel serve .html --open",
    "start": "parcel serve index.html --open"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.1",
    "parcel": "^2.0.1"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.1",
    "leaflet": "^1.7.1"
  },
  "alias": {
    "leaflet": {
      "global": "L"
    },
    "bootstrap": {
      "global": "bootstrap"
    }
  }
}
